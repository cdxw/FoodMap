        app:layout_constraintTop_toTopOf="@+id/sugars_txt" />

    <TextView
        android:id="@+id/textView37"

        android:layout_width="55dp"
        android:layout_height="22dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="-3dp"
