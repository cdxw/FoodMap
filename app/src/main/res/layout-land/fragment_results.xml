    <TextView
        android:id="@+id/polyFat_txt"
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="-1dp"
        android:text="@string/zero_default"
        android:textAlignment="textStart"
        android:textColor="@android:color/holo_orange_dark"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@+id/textView22"
        app:layout_constraintTop_toTopOf="@+id/textView22" />

    <TextView
        android:id="@+id/monoFat_txt"
        android:layout_width="43dp"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="-1dp"
        android:text="@string/zero_default"
        android:textAlignment="textStart"
        android:textColor="@android:color/holo_orange_dark"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@+id/textView18"
        app:layout_constraintTop_toTopOf="@+id/textView18" />

    <TextView
        android:id="@+id/textView16"

        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="0dp"
        android:text="@string/fats_total"
        android:textAlignment="textStart"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        app:layout_constraintLeft_toRightOf="@+id/calories_txt"
        app:layout_constraintTop_toTopOf="@+id/calories_txt" />

    <TextView
        android:id="@+id/textView24"

        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="5dp"
        android:text="@string/cholesterol"
        android:textAlignment="textStart"
        android:textSize="18sp"
        android:textStyle="italic"
